{
    "manifest_version": 3,
    "name": "Favorite-Blablob!",
    "description": "Blablob tri vos favoris !",
    "version": "1.0",
    "action": {
        "default_popup": "front/index.html",
        "default_icon": "images/blablu.png"
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "permissions": [
        "bookmarks",
        "storage"
    ],
    "host_permissions": [
        "http://127.0.0.1:11434/*"
    ],
    "icons": {
        "16": "images/blablu.png",
        "48": "images/blablu.png",
        "128": "images/blablu.png"
    }
}